{
  "expo": {
    "name": "wallstreetstocksapp",
    "slug": "wallstreetstocks-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/wallstreetstocks.png",
    "scheme": "wallstreetstocksapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "ios": {
      "bundleIdentifier": "ai.wallstreetstocks.app",
      "supportsTablet": true,
      "infoPlist": {
        "NSAppleEventsUsageDescription": "This app uses Apple Sign-In for authentication.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "com.apple.developer.applesignin": [
          "Default",
          "Default"
        ]
      }
    },
    "android": {
      "package": "ai.wallstreetstocks.app",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/wallstreetstocks.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "apiUrl": "https://www.wallstreetstocks.ai/api",
      "expoClientId": "596401606956-k2basop69e3nib00a4de4hbv2mbkcrvp.apps.googleusercontent.com",
      "router": {},
      "eas": {
        "projectId": "abc189cc-a5e1-470c-9536-2bf078c4fb73"
      }
    }
  }
}

